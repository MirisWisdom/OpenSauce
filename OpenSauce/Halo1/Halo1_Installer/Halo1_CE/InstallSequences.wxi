	</Custom>

	<!-- Patch OS_Sapien-->
	<Custom Action='Halo1_CE_SetFilePatcherIDSapien_CACT' After='Halo1_CE_PatchOSTool_CACT'>
		<![CDATA[(NOT HALO1_CE_HEK_SKIPCREATEOSEXES_PROP) AND((&Halo1_CE_HEK_FEAT = 3) AND NOT (!Halo1_CE_HEK_FEAT = 3)) AND HALO1_CE_HEK_SAPIENPATH_PROP]]>
