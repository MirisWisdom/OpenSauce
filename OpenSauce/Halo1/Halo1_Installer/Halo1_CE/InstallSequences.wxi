		<![CDATA[((&Halo1_CE_ClientServer_FEAT = 3) AND NOT (!Halo1_CE_ClientServer_FEAT = 3))]]>
	</Custom>
	<Custom Action='Halo1_CE_PatchOSHaloCEDed_CACT' After='Halo1_CE_SetFilePatcherOutPathHaloCEDed_CACT'>
		<![CDATA[((&Halo1_CE_ClientServer_FEAT = 3) AND NOT (!Halo1_CE_ClientServer_FEAT = 3))]]>
	</Custom>

	<!-- Patch OS_Guerilla -->
	<Custom Action='Halo1_CE_SetFilePatcherIDGuerilla_CACT' After='Halo1_CE_PatchOSHaloCEDed_CACT'>
		<![CDATA[(NOT HALO1_CE_HEK_SKIPCREATEOSEXES_PROP) AND((&Halo1_CE_HEK_FEAT = 3) AND NOT (!Halo1_CE_HEK_FEAT = 3)) AND HALO1_CE_HEK_GUERILLAPATH_PROP]]>
