<?xml version="1.0"?>

<!--YeloDebug Settings-->
<Settings>

  <!--The version of this settings file-->
  <Version>1.0.0.4</Version>

  <!--If the xdk is installed on your computer it will create this registry entry to store the last connection used-->
  <XdkRegistryPath>HKEY_CURRENT_USER\Software\Microsoft\XboxSDK</XdkRegistryPath>

  <!--This is the temporary directory that the searcher uses for saving its information-->
  <SearcherPath>C:\WINDOWS\Temp\Searcher\</SearcherPath>
  
  <!--Set to true if you wish to mess with breakpoints and other junk that requires 
  a notification session to be established-->
  <NotificationSession>false</NotificationSession>

  <!--Change if you are having problems establishing a notification session-->
  <NotificationPort>731</NotificationPort>
  
</Settings>