    mul r1.x, r0.w, c9.z
    max r2.x, r0.w, r1.x
    dp3 r2.y, -c20, t3
    max r1.xy, r2, t2.z
    min r2.xy, t2.w, r1
    mad_pp r0.xyz, r2.x, c19, r0
